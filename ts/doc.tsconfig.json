{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.tsconfig.json",
  "include": [
    "../src"
  ],
  "compilerOptions": {
    "sourceMap": false,
    "sourceRoot": ""
  },
  "typedocOptions": {
    "out": "../docs-dist",
    "excludePrivate": true,
    "excludeInternal": true,
    "theme": "default",
    "name": "True Myth",
    "entryPointStrategy": "expand",
    "readme": "../README.md",
    "includeVersion": true,
    "githubPages": true,
    "useTsLinkResolution": true
  }
}
